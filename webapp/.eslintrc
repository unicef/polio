{
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "mocha": true
  },
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "globals": {
    "mountNode": true
  },
  "ecmaFeatures": {
    "arrowFunctions": true,
    "destructuring": true,
    "defaultParams": true,
    "jsx": true
  },
  "rules": {
    "curly": 0,
    "comma-dangle": [
      2,
      "never"
    ],
    "comma-spacing": 0,
    "eqeqeq": [
      2,
      "allow-null"
    ],
    "key-spacing": 0,
    "no-underscore-dangle": 0,
    "no-unused-expressions": 0,
    "no-shadow": 0,
    "no-shadow-restricted-names": 0,
    "no-extend-native": 0,
    "no-var": 2,
    "strict": 0,
    "no-use-before-define": 0,
    "new-cap": 0,
    "eol-last": 0,
    "quotes": [
      2,
      "single"
    ],
    "semi": 0,
    "space-unary-ops": 0,
    "space-infix-ops": 0,
    "consistent-return": 0,
    "handle-callback-err": [
      2,
      "^(err|error|anySpecificError)$"
    ],
    "react/jsx-boolean-value": 1,
    "react/jsx-no-undef": 1,
    "react/jsx-quotes": 1,
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1,
    "react/no-danger": 1,
    "react/no-did-mount-set-state": 1,
    "react/no-did-update-set-state": 1,
    "react/no-multi-comp": 1,
    "react/no-unknown-property": 1,
    "react/prop-types": 1,
    "react/react-in-jsx-scope": 1,
    "react/require-extension": 1,
    "react/self-closing-comp": 1,
    "react/sort-comp": 1,
    "react/wrap-multilines": 1
  }
}
